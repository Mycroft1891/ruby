<h1>Usuarios (<%= @users.count %>)</h1>
  <div class="row col-md-offset-3 col-sm-offset-3">
    <ul class="users col-sm-8 col-md-offset-2 col-sm-8 col-sm-offset-2 col-xs-12">
      <%= render @users %>
      <%= raw @ad.banner_ad %>
      <div id="infinite-scrolling">
        <%= will_paginate @users, :class => "page" %>
      </div>
    </ul>
  </div>
